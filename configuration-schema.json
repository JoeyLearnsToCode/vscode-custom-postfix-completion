{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Generated schema for Root",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "language": {
                "type": "string",
                "description": "language name"
            },
            "templates": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "triggerWord": {
                            "type": "string"
                        },
                        "description": {
                            "type": "string"
                        },
                        "contents": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    },
                    "required": [
                        "triggerWord",
                        "description",
                        "contents"
                    ]
                }
            }
        },
        "required": [
            "language",
            "templates"
        ]
    }
}